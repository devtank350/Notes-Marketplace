                    <li><a href="#contact" class="smooth-scroll">Setting</a></li>
                    <li>
                        <a href="#contact" class="smooth-scroll collapsible">
                            <img src="~/Content/images/Dashboard/user-img.png" class="img-circle" id="header-img">
                        </a>
                    </li>
                    <li><a href="#services" class="smooth-scroll"><button type="button" class="btn btn-primary" id="header-bt">LOGOUT</button></a></li>
                </ul>
            </div>
        </div>
        <div id="mobile-nav">
            <span id="mobile-nav-close-btn">
                &times;
            </span>
            <div id='mobile-nav-content'>
                <ul class="nav">
                    <li><a href="search.html" class="smooth-scroll">Dashboard</a></li>
                    <li><a href="#about" class="smooth-scroll">Notes</a></li>
                    <li><a href="#about" class="smooth-scroll">Members</a></li>
                    <li><a href="#team" class="smooth-scroll">Reports</a></li>
                    <li><a href="#contact" class="smooth-scroll">Setting</a></li>
                    <li>
                        <a href="#contact" class="smooth-scroll collapsible">
                            <img src="~/Content/images/Dashboard/user-img.png" class="img-circle" id="header-img">
                        </a>
                    </li>
                    <li><a href="#services" class="smooth-scroll"><button type="button" class="btn btn-primary" id="header-bt">LOGOUT</button></a></li>
                </ul>
            </div>
        </div>
    </header>
    <section id="admin-profile">
        <div class="content-box-md">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <h4>
                                Add Country
                            </h4>
                            @using (Html.BeginForm("AddCountry", "AdminSettings", FormMethod.Post))
                            {

                        @Html.AntiForgeryToken()
                        <div class="form-group signup-value">
                            <label>Country Name</label><span class="required">*</span>
                            @*<input type="text" class="form-control" id="exampleInputtext1" aria-describedby="textHelp" placeholder="Enter Your First Name">*@
                            @Html.TextBoxFor(model => model.CountryName, new { @class = "form-control", id = "country-name", placeholder = "Enter country name", required = "required" })
                            @Html.ValidationMessageFor(model => model.CountryName, "", new { @class = "text-invalid" })
                        </div>
                        <div class="form-group signup-value">
                            <label>Country Code</label><span class="required">*</span>
                            @*<input type="text" class="form-control" id="exampleInputtext1" aria-describedby="textHelp" placeholder="Enter Your First Name">*@
                            @Html.TextBoxFor(model => model.CountryCode, new { @class = "form-control", id = "country-code", placeholder = "Enter country code", required = "required" })
                            @Html.ValidationMessageFor(model => model.CountryCode, "", new { @class = "text-invalid" })
                        </div>
                        <button type="submit" class="btn btn-primary" id="admin-p-btn">SUBMIT</button>
                            }

                        </div>


                    </div>

                </div>
            </div>
        </div>

    </section>







